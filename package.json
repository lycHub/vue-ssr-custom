{
  "name": "vue-ssr-custom",
  "version": "1.0.0",
  "description": "vue ssr",
  "main": "index.js",
  "scripts": {
    "start:spa": "webpack serve --config configs/client/index.js",
    "start:ssr": "concurrently \"npm run start:spa\" \"nodemon server\"",
    "build:client": "webpack --env production --config configs/client",
    "build:server": "webpack --config configs/server.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "server": "node server"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/Madom/vue-ssr-custom.git"
  },
  "keywords": [
    "vue",
    "ssr"
  ],
  "author": "zgcf",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@vue/babel-preset-app": "^4.5.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.1",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^1.3.6",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "vue": "^2.6.12",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.1",
    "vue-server-renderer": "^2.6.12"
  }
}
